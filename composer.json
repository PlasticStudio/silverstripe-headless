{
  "name": "silverstripe/silverstripe-headless",
  "description": "Silverstripe CMS headless features",
  "type": "silverstripe-vendormodule",
  "license": "MIT",
  "keywords": [
    "silverstripe",
    "headless"
  ],
  "authors": [
    {
      "name": "Uncle Cheese"
    }
  ],
  "require": {
    "silverstripe/cms": "^4.4 || ^5",
    "silverstripe/graphql": "^4 || ^5",
    "silverstripe/cms-events": "*",
    "unclecheese/display-logic": "~2.0 || ^3",
    "sminnee/silverstripe-apikey": "dev-feature/update",
    "symbiote/silverstripe-grouped-cms-menu": "^4",
    "firebase/php-jwt": "^5.4",
    "guzzlehttp/guzzle": "^7.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/PlasticStudio/silverstripe-apikey.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "SilverStripe\\Headless\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
